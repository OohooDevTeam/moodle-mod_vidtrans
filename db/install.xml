<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/vidtrans/db" VERSION="20101203" COMMENT="XMLDB file for Moodle mod/vidtrans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="vidtrans" COMMENT="Default comment for vidtrans, please edit me"  NEXT="vidtrans_languages">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
                <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Course vidtrans activity belongs to" PREVIOUS="id" NEXT="name"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="name field for moodle instances" PREVIOUS="course" NEXT="intro"/>
                <FIELD NAME="intro" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="General introduction of the vidtrans activity" PREVIOUS="name" NEXT="introformat"/>
                <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)" PREVIOUS="intro" NEXT="timecreated"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="introformat" NEXT="timemodified"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timecreated"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="vidtrans_languages" COMMENT="" PREVIOUS="vidtrans">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
                <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Course vidtrans activity belongs to" PREVIOUS="id" NEXT="french"/>
                <FIELD NAME="french" TYPE="int" NOTNULL="true" SEQUENCE="false" PREVIOUS="course" NEXT="spanish"/>
                <FIELD NAME="spanish" TYPE="int" NOTNULL="true" SEQUENCE="false" PREVIOUS="french" NEXT="russian"/>  
                <FIELD NAME="russian" TYPE="int" NOTNULL="true" SEQUENCE="false" PREVIOUS="spanish" NEXT="japanese"/>  
                <FIELD NAME="japanese" TYPE="int" NOTNULL="true" SEQUENCE="false" PREVIOUS="russian"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
            </INDEXES>
        </TABLE>
    </TABLES>
</XMLDB>
